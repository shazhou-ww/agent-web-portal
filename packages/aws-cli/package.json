{
  "name": "@agent-web-portal/aws-cli",
  "version": "0.1.0",
  "description": "CLI tools for Agent Web Portal AWS deployment",
  "license": "MIT",
  "author": "Agent Web Portal Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/agent-web-portal.git",
    "directory": "packages/aws-cli"
  },
  "homepage": "https://github.com/anthropics/agent-web-portal#readme",
  "bugs": {
    "url": "https://github.com/anthropics/agent-web-portal/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mcp",
    "ai-agent",
    "agent-web-portal",
    "awp",
    "aws",
    "cli",
    "deploy"
  ],
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "bin": {
    "awp": "./bin/awp.ts"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "bin",
    "README.md"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "tsc --noEmit && biome check ."
  },
  "dependencies": {
    "@agent-web-portal/core": "workspace:*",
    "@aws-sdk/client-s3": "^3.700.0",
    "archiver": "^7.0.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/node": "^22.10.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
