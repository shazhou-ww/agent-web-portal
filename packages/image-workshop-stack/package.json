{
  "name": "@agent-web-portal/image-workshop-stack",
  "version": "0.1.0",
  "private": true,
  "description": "Image Workshop service with AI image generation and editing tools",
  "type": "module",
  "main": "dist/handler.mjs",
  "scripts": {
    "start": "bun run server.ts",
    "dev": "bun run server.ts",
    "dev:sam": "bun run build && sam build && sam local start-api --port 3600",
    "clean": "rimraf .turbo dist .aws-sam",
    "build": "bun build src/handler.ts --outdir dist --format esm --target node --external '@aws-sdk/*' && mv dist/handler.js dist/handler.mjs",
    "sam:build": "bun run build && sam build",
    "sam:local": "sam local start-api --port 3600",
    "sam:deploy": "sam deploy",
    "sam:remove": "sam delete",
    "deploy": "bun run sam:build && bun run sam:deploy",
    "deploy:idp": "bun run sam:build && bun run scripts/deploy-with-idp.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "tsc --noEmit && biome check ."
  },
  "dependencies": {
    "@agent-web-portal/auth": "workspace:*",
    "@agent-web-portal/awp-server-core": "workspace:*",
    "@agent-web-portal/awp-server-lambda": "workspace:*",
    "@agent-web-portal/cas-client-core": "workspace:*",
    "@aws-sdk/client-cognito-identity-provider": "^3.975.0",
    "@aws-sdk/client-dynamodb": "^3.975.0",
    "@aws-sdk/client-secrets-manager": "^3.975.0",
    "@aws-sdk/lib-dynamodb": "^3.975.0",
    "jose": "^6.1.3",
    "zod": "3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/bun": "latest",
    "@types/node": "^22.10.5"
  }
}
