{
  "name": "@agent-web-portal/examples",
  "version": "0.1.0",
  "private": true,
  "description": "Example servers and E2E tests for Agent Web Portal",
  "type": "module",
  "scripts": {
    "clean": "rimraf .turbo dist",
    "dev": "bun run --watch server.ts",
    "start": "bun run server.ts",
    "test": "bun test",
    "test:e2e": "bun test e2e.test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "tsc --noEmit && biome check ."
  },
  "dependencies": {
    "@agent-web-portal/core": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
