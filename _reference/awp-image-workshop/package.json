{
  "name": "@mitsein/awp-image-workshop",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=es2022 --outdir=dist --format=esm --external:@aws-sdk/* --loader:.md=text",
    "typecheck": "tsc --noEmit 2>&1 || echo 'Note: node_modules errors from AWP packages are expected'",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --write",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@agent-web-portal/core": "^0.2.0",
    "@agent-web-portal/aws-lambda": "^0.2.0",
    "@aws-sdk/client-secrets-manager": "^3.700.0",
    "@aws-sdk/client-ssm": "^3.700.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  }
}
