{
  "name": "@agent-web-portal/casfa-v2",
  "version": "0.1.0",
  "private": true,
  "description": "CASFA v2 - Content-Addressable Storage for Agents (refactored)",
  "type": "module",
  "main": "backend/dist/handler.mjs",
  "scripts": {
    "dev": "bun run backend/server.ts",
    "dev:setup": "bun run backend/scripts/dev-setup.ts",
    "clean": "rimraf .turbo backend/dist .aws-sam",
    "build": "bun run build:backend",
    "build:backend": "bun run backend/scripts/build.ts",
    "sam:build": "bun run build:backend && sam build",
    "sam:local": "sam local start-api --port 3560",
    "sam:deploy": "sam deploy",
    "test": "bun test",
    "test:e2e": "bun run backend/scripts/integration-test.ts",
    "test:e2e:debug": "bun run backend/scripts/integration-test.ts --no-cleanup",
    "db:create": "bun run backend/scripts/create-local-tables.ts",
    "db:delete": "bun run backend/scripts/create-local-tables.ts --delete",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "tsc --noEmit && biome check ."
  },
  "dependencies": {
    "@agent-web-portal/auth": "workspace:*",
    "@agent-web-portal/cas-core": "workspace:*",
    "@agent-web-portal/casfa-protocol": "workspace:*",
    "@agent-web-portal/cas-storage-core": "workspace:*",
    "@agent-web-portal/cas-storage-fs": "workspace:*",
    "@agent-web-portal/cas-storage-memory": "workspace:*",
    "@agent-web-portal/cas-storage-s3": "workspace:*",
    "@aws-sdk/client-cognito-identity-provider": "^3.975.0",
    "@aws-sdk/client-dynamodb": "^3.975.0",
    "@aws-sdk/lib-dynamodb": "^3.975.0",
    "@hono/zod-validator": "^0.4.1",
    "amazon-cognito-identity-js": "^6.3.12",
    "hono": "^4.6.0",
    "jose": "^6.1.3",
    "xxhash-wasm": "^1.1.0",
    "zod": "3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/bun": "latest",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2"
  }
}
