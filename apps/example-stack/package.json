{
  "name": "@agent-web-portal/example-stack",
  "version": "0.2.0",
  "private": true,
  "description": "Example AWS Lambda stack for Agent Web Portal",
  "type": "module",
  "main": "dist/handler.mjs",
  "scripts": {
    "start": "bun run server.ts",
    "dev": "bun run server.ts",
    "dev:sam": "bun run build && sam build && sam local start-api --port 3456 --env-vars env.json",
    "clean": "rimraf .turbo dist .aws-sam .skill-cache",
    "build": "bun build src/handler.ts --outdir dist --format esm --target node --external '@aws-sdk/*' && mv dist/handler.js dist/handler.mjs",
    "sam:build": "bun run build && sam build",
    "sam:local": "sam local start-api --port 3456 --env-vars env.json",
    "sam:deploy": "sam deploy",
    "sam:remove": "sam delete",
    "deploy:skills": "bun run scripts/upload-skills.ts --stack",
    "deploy:skills:local": "bun run scripts/upload-skills.ts --local",
    "deploy": "bun run sam:build && bun run sam:deploy && bun run deploy:skills",
    "test": "bun test",
    "test:e2e": "bun test e2e.test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "tsc --noEmit && biome check ."
  },
  "dependencies": {
    "@agent-web-portal/auth": "workspace:*",
    "@agent-web-portal/aws-lambda": "workspace:*",
    "@agent-web-portal/client": "workspace:*",
    "@agent-web-portal/core": "workspace:*",
    "@aws-sdk/client-cloudformation": "^3.975.0",
    "@aws-sdk/client-cloudfront": "^3.975.0",
    "@aws-sdk/client-dynamodb": "^3.975.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/client-secrets-manager": "^3.975.0",
    "@aws-sdk/client-ssm": "^3.975.0",
    "@aws-sdk/credential-providers": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "jsonata": "^2.0.5",
    "jszip": "^3.10.1",
    "setimmediate": "^1.0.5",
    "zod": "3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/bun": "latest",
    "@types/node": "^22.10.5"
  }
}
